{
  "title": "Mac Mouse 4/5 desktop switcher",
  "rules": [
    {
      "description": "Mouse 4 Switch Left",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button4"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "left_arrow",
              "modifiers": [
                "left_control",
		"left_command",
		"left_option",
		"left_shift"

              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Mouse 5 Switch Right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button5"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "right_arrow",
              "modifiers": [
                "left_control",
		"left_command",
		"left_option",
		"left_shift"
              ]
            }
          ]
        }
      ]
    }
  ]
}
